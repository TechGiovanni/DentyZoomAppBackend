{
  "name": "backend",
  "version": "1.0.0",
  "description": "App to socialize. My favorite backend app",
  "main": "app.js",
  "scripts": {
    "dev:start": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "dev:conc": "concurrently \"ttsc -p tsconfig.prod.json --watch\" \"nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan\"",
    "dev:build": "rm -rf build && prettier --write src/ && twc -w",
    "dev:nodemon": "nodemon | ./node_modules/.bin/bunyan",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --jestcoverage",
    "build": "ttsc -p tsconfig.prod.json",
    "prod": "NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register build/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.0.1",
    "axios": "^1.2.3",
    "cloudinary": "^1.33.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.8.4",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.4",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "bunyan": "^1.8.15",
    "colors": "^1.4.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
